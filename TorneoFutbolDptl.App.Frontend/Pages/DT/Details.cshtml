@page
@model TorneoFutbolDptl.App.Frontend.Pages.DT.DetailsModel
@{
}
<h1>Detalles del Director Técnico</h1>
<table class="table table-striped">
    <tr>
        <th scope="col">Nombre</th>
        <td>@Model.directorTecnico.Nombre</td>
    </tr>
    <tr>
        <th scope="col">Documento</th>
        <td>@Model.directorTecnico.Documento</td>
    </tr>
    <tr>
        <th scope="col">Numero Telefono</th>
        <td>@Model.directorTecnico.NumeroTelefono</td>
    </tr>
    </table>
    <a class="btn btn-primary" asp-page="./Edit" asp-route-id="@Model.directorTecnico.Id">Editar</a>
</table>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>