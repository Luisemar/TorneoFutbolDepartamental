@page
@model TorneoFutbolDptl.App.Frontend.Pages.DT.IndexModel
@{
}
<h1> Listado de Directores Tecnicos</h1>
<br>
<a class="btn btn-primary" asp-page="./Create">Crear DT</a>
<p></p>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Documento</th>
            <th scope="col">NumeroTelefono</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var directorTecnico in Model.directorTecnicos)
        {
            <tr>
                <td>@directorTecnico.Nombre</td>
                <td>@directorTecnico.Documento</td>
                <td>@directorTecnico.NumeroTelefono</td>

            </tr>
        }
    </tbody>
</table>
