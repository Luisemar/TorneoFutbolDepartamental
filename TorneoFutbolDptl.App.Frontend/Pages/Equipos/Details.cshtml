@page
@model TorneoFutbolDptl.App.Frontend.Pages.Equipos.DetailsModel
@{
}
<h1>Detalles del Equipo</h1>
<table class="table table-striped">
    <tr>
        <th scope="col">Nombre</th>
        <td>@Model.equipo.Nombre</td>
    </tr>
    <tr>
        <th scope="col">Municipio</th>
        <td>@Model.equipo.Municipio.Nombre</td>
    </tr>
    <tr>
        <th scope="col">Director Tecnico</th>
        <td>@Model.equipo.DirectorTecnico</td>
    </tr>
    </table>
    <a class="btn btn-primary" asp-page="./Edit" asp-route-id="@Model.equipo.Id">Editar</a>
</table>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>